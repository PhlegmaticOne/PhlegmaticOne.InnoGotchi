@{
    Layout = "_Layout";
}

<div class="row">
    <div class="col-lg-9">
        @RenderBody()
    </div>
    <div class="col-lg-3">
        @await RenderSectionAsync("ColumnContent")
    </div>
</div>

@section Scripts
{
    @await RenderSectionAsync("Scripts", required: false)
}

@section Styles
{
    @await RenderSectionAsync("Styles", required: false)
}
